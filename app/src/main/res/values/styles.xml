 <resources>
    <!-- Base application theme. -->
    <style parent="Theme.AppCompat.Light.DarkActionBar" name="AppTheme">
        <!-- Customize your theme here. -->
        <item name="colorPrimary">@color/colorPrimary</item>
        <item name="colorPrimaryDark">@color/colorPrimaryDark</item>
        <item name="colorAccent">@color/colorAccent</item>
    </style>
    <style name="AppTheme.NoActionBar">
        <item name="windowActionBar">false</item>
        <item name="windowNoTitle">true</item>
    </style>
    <style parent="ThemeOverlay.AppCompat.Dark.ActionBar" name="AppTheme.AppBarOverlay"/>
    <style parent="ThemeOverlay.AppCompat.Light" name="AppTheme.PopupOverlay"/>
    <style parent="Theme.AppCompat.Light.DarkActionBar" name="MyBaseAppTheme">
        <item name="android:textViewStyle">@style/AppMediumText</item>
    </style>
</resources>

